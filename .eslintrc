{
  "parser": "babel-eslint",
  "rules": {
    "global-strict": 0,
    "strict": [2, "global"],
    "quotes": 0,
    "no-mixed-requires": 0,
    "no-unused-expressions": 0,
    "eol-last": 0,
    "no-underscore-dangle": 0,
    "new-cap": 0,
    "indent": [2, 2],
    "no-unused-vars": 1,
    "no-trailing-spaces": 1
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "plugins": [
    "react"
  ]
}